<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:centerX="0.3"
                android:startColor="#D9D9D9"
                android:endColor="#666666"
                android:angle="270" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:angle="180"
                android:centerX="0.9"
                android:endColor="#FF404040"
                android:startColor="#33C7C7C7" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:angle="360"
                android:centerX="0.9"
                android:endColor="#FF404040"
                android:startColor="#33C7C7C7" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:angle="-90"
                android:centerY="0.9"
                android:endColor="#FF5C5C5C"
                android:startColor="#33C7C7C7" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:angle="90"
                android:centerY="0.9"
                android:endColor="#FF5C5C5C"
                android:startColor="#33C7C7C7" />
        </shape>
    </item>
</layer-list>