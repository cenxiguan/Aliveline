<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:startColor="#949494"
                android:endColor="#3B3B3B"
                android:angle="225" />
            <stroke
                android:width="2dp"
                android:color="#1C1C1C" />
        </shape>
    </item>

    <item>
        <shape>
            <gradient
                android:angle="180"
                android:centerColor="#00595959"
                android:centerX="0.95"
                android:endColor="#99000000"
                android:startColor="#00595959" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:angle="360"
                android:centerColor="#00595959"
                android:centerX="0.95"
                android:endColor="#99000000"
                android:startColor="#00595959" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:angle="90"
                android:centerColor="#00595959"
                android:centerX="0.95"
                android:endColor="#99000000"
                android:startColor="#00595959" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:angle="-90"
                android:centerColor="#00595959"
                android:centerX="0.95"
                android:endColor="#99000000"
                android:startColor="#00595959" />
        </shape>
    </item>
</layer-list>
